{
  "name": "@blogs/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "blogs:dev": "nx run blogs:serve",
    "backend:dev": "nx run backend:serve",
    "backend:test": "nx run backend:test",
    "backend:build": "nx run backend:build",
    "backend:build-docker": "nx run backend:docker-build",
    "codegen": "ts-node --project tools/tsconfig.tools.json -r tsconfig-paths/register tools/bin/codegen.ts",
    "codegen-client": "graphql-codegen --config graphql-codegen.ts",
    "genql": "ts-node --project tools/tsconfig.tools.json -r tsconfig-paths/register tools/bin/genql.ts",
    "setup:backend": "yarn && yarn db-primary:generate && yarn codegen && yarn genql",
    "setup:database": "yarn db-primary:migrate && yarn db-primary:seed",
    "db-primary:pull": "prisma db pull --schema prisma/primary/schema.prisma",
    "db-primary:reset": "prisma migrate reset --schema prisma/primary/schema.prisma",
    "db-primary:migrate": "prisma migrate dev --schema prisma/primary/schema.prisma",
    "db-primary:deploy": "prisma migrate deploy --schema prisma/primary/schema.prisma",
    "db-primary:generate": "prisma generate --schema prisma/primary/schema.prisma",
    "db-primary:seed": "ts-node prisma/primary/seed/index.ts",
    "db-primary:studio": "prisma studio --schema prisma/primary/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.9.7",
